  /usr/share/doc/gumbo-parser/maintaining.md
  /usr/share/doc/gumbo-parser/testing.md

(libgumbo2)
  /usr/bin/cyggumbo-2.dll

(libgumbo-devel)
  /usr/include/gumbo.h
  /usr/include/tag_enum.h
  /usr/lib/libgumbo.dll.a
