  /usr/bin/cygi3ipc-glib-1.0-0.dll

(libi3ipc-glib1.0-devel)
  /usr/include/i3ipc-glib/i3ipc-con.h
  /usr/include/i3ipc-glib/i3ipc-connection.h
  /usr/include/i3ipc-glib/i3ipc-enum-types.h
  /usr/include/i3ipc-glib/i3ipc-event-types.h
  /usr/include/i3ipc-glib/i3ipc-glib.h
  /usr/include/i3ipc-glib/i3ipc-reply-types.h
  /usr/lib/libi3ipc-glib-1.0.dll.a
  /usr/lib/pkgconfig/i3ipc-glib-1.0.pc

(girepository-i3ipc1.0)
  /usr/lib/girepository-1.0/i3ipc-1.0.typelib
  /usr/share/gir-1.0/i3ipc-1.0.gir

