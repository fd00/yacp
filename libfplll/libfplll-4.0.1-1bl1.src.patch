+    {
+      if (x == L_(0.0))
+        return FP_ILOGB0;
+      else
+        {
+          int e;
+
+          (void) FREXP (x, &e);
+          return e - 1;
+        }
+    }
+  else
+    {
+      if (ISNAN (x))
+        return FP_ILOGBNAN;
+      else
+        return INT_MAX;
+    }
+}
