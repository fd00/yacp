  /usr/share/doc/libfprint/MAINTAINERS
  /usr/share/doc/libfprint/NEWS
  /usr/share/doc/libfprint/README.md
  /usr/share/doc/libfprint/THANKS
  /usr/share/icons/org.freedesktop.libfprint.Demo.png
  /usr/share/metainfo/org.freedesktop.libfprint.Demo.appdata.xml
  /usr/share/metainfo/org.freedesktop.libfprint.metainfo.xml

(libfprint2_2)
  /usr/bin/cygfprint-2-2.dll

(libfprint2-devel)
  /usr/include/libfprint-2/fp-context.h
  /usr/include/libfprint-2/fp-device.h
  /usr/include/libfprint-2/fp-enums.h
  /usr/include/libfprint-2/fp-image-device.h
  /usr/include/libfprint-2/fp-image.h
  /usr/include/libfprint-2/fp-print.h
  /usr/include/libfprint-2/fprint.h
  /usr/lib/libfprint-2.dll.a
  /usr/lib/pkgconfig/libfprint-2.pc

(girepository-FPrint2.0)
  /usr/lib/girepository-1.0/FPrint-2.0.typelib
  /usr/share/gir-1.0/FPrint-2.0.gir

