  /usr/include/homegear-ipc/IQueueBase.h
  /usr/include/homegear-ipc/IpcException.h
  /usr/include/homegear-ipc/IpcResponse.h
  /usr/include/homegear-ipc/JsonDecoder.h
  /usr/include/homegear-ipc/JsonEncoder.h
  /usr/include/homegear-ipc/Math.h
  /usr/include/homegear-ipc/Output.h
