  /usr/share/doc/libkdumpfile/COPYING.GPLv2
  /usr/share/doc/libkdumpfile/COPYING.GPLv3
  /usr/share/doc/libkdumpfile/COPYING.LGPLv3
  /usr/share/doc/libkdumpfile/NEWS
  /usr/share/doc/libkdumpfile/README.md
  /usr/share/doc/libkdumpfile/attributes.md
  /usr/share/doc/libkdumpfile/objects.md
  /usr/share/doc/libkdumpfile/threads.md

(libkdumpfile12)
  /usr/bin/cygkdumpfile-12.dll

(libkdumpfile-devel)
  /usr/include/libkdumpfile/kdumpfile.h
  /usr/lib/libkdumpfile.dll.a
  /usr/lib/pkgconfig/libkdumpfile.pc

(libaddrxlat3)
  /usr/bin/cygaddrxlat-3.dll

(libaddrxlat-devel)
  /usr/include/libkdumpfile/addrxlat.h
  /usr/lib/libaddrxlat.dll.a
  /usr/lib/pkgconfig/libaddrxlat.pc

