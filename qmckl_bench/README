  /usr/share/qmckl_bench/Alz_large.h5
  /usr/share/qmckl_bench/Alz_large.png
  /usr/share/qmckl_bench/Alz_small.h
  /usr/share/qmckl_bench/Alz_small.h5
  /usr/share/qmckl_bench/Alz_small.png
  /usr/share/qmckl_bench/Sildenafil.h5
  /usr/share/qmckl_bench/Sildenafil.png
