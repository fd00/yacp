  /usr/share/doc/vala-dbus-binding-tool/COPYING
  /usr/share/doc/vala-dbus-binding-tool/ChangeLog
  /usr/share/doc/vala-dbus-binding-tool/MAINTAINERS
  /usr/share/doc/vala-dbus-binding-tool/NEWS
  /usr/share/doc/vala-dbus-binding-tool/README
  /usr/share/doc/vala-dbus-binding-tool/README.md
  /usr/share/man/man1/vala-dbus-binding-tool.1.gz

